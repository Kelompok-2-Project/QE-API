{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "message": {
      "type": "string"
    },
    "data": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "id": {
              "type": "integer"
            },
            "name": {
              "type": "string"
            },
            "category": {
              "type": "string"
            },
            "price": {
              "type": "integer"
            },
            "photo_product": {
              "type": "string"
            }
          },
          "required": [
            "id",
            "name",
            "category",
            "price",
            "photo_product"
          ]
        }
      ]
    }
  },
  "required": [
    "message",
    "data"
  ]
}